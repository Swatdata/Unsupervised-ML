columns[2]]\n","\n","    # Calculate the euclidean distance between student_a and student_b\n","    e_dist = ((student_a_food1 - student_b_food1)**2 + (student_a_food2 - student_b_food2)**2 + (student_a_food3 - student_b_food3)**2)**0.5\n","\n","    # Store this single euclidean distance in the list student_a_distances\n","    student_a_distances.append(e_dist)\n","\n","  # Store the list of student_a_distances in the list all_distances\n","  all_distances.append(student_a_distances)\n","\n","# Make a DataFrame from all_distances\n","euclidean_df = pd.DataFrame(all_distances, index=df.index, columns=df.index)\n","\n","# Show the DataFrame created\n","euclidean_df"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":457},"id":"Cwku6oICZIDA","executionInfo":{"status":"ok","timestamp":1663930663312,"user_tz":-120,"elapsed":22,"user":{"displayName":"Ben Elvin","userId":"09111547648527423041"}},"outputId":"3d490cd8-28a4-4b76-9bda-5131e070fc66"},"id":"Cwku6oICZIDA","execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["student          Akbar   Andreas     Dónal       Hana  Konstantinos  \\\n","student                                                               \n","Akbar         0.000000  7.280110  6.782330   9.899495      8.062258   \n","Andreas       7.280110  0.000000  1.732051   5.196152      6.000000   \n","Dónal         6.782330  1.732051  0.000000   6.324555      7.141428   \n","Hana          9.899495  5.196152  6.324555   0.000000      8.660254   \n","Konstantinos  8.062258  6.000000  7.141428   8.660254      0.000000   \n","Kristina      9.643651  7.348469  8.306624  10.630146      2.449490   \n","Sabrina       5.099020  7.681146  6.633250   9.165151     11.445523   \n","Sandrine      4.690416  5.916080  4.472136  10.000000      9.110434   \n","Sevgi         4.582576  5.477226  4.582576   7.280110      9.486833   \n","Rias          6.782330  1.732051  0.000000   6.324555      7.141428   \n","Ben           8.0622